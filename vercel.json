{
  "version": 2,
  "routes": [
    {
      "src": "/preview/([^/]+)",
      "dest": "/api/preview.js?username=$1"
    },
    {
      "src": "/postpreview/([^/]+)",
      "dest": "/api/postpreview/$1.js"
    }
  ],
  "crons": [
    {
      "path": "/api/daily-slack",
      "schedule": "0 9 * * *"
    },
    {
      "path": "/api/ingest",
      "schedule": "0 3 * * 0"
    }
  ]
}